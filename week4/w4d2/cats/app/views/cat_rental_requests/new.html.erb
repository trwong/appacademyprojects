<h1>Cat Rental Requests</h1>



<form action="cat_rental_requests_url" method="post">

  <label for="cat">Cat</label>
  <select id="cat" name="cat_rental_request[:id]">
    <option disabled>--Select Cat--</option>
    <%= Cat.all.each do |cat| %>
      <option value="<%= cat.id %>" > <%= cat.name %> </option>
    <% end %>
  </select>
<br>
  <label for="start_date">Start Date</label>
  <input type="date" name="cat_rental_request[start_date]" value="">
  <br>
  <label for="end_date">End Date</label>
  <input type="date" name="cat_rental_request[end_date]" value="">
<br>
  <input type="submit" value="Submit">
</form>

<br>

<%= button_to 'Cats', cats_url, method: :get %>
