<h2>Sign Up</h2>

<form action="<%= users_url %>" method="post">
  <input
  type="hidden"
  name="authenticity_token"
  value="<%= form_authenticity_token %>">
  <label for="user_email">Email:</label>
  <input
    id="user_email"
    type="text"
    name="user[email]">
  <br>
  <label for="user_password">Password:</label>
  <input
    id="user_password"
    type="password"
    name="user[password]">
  <br>
  <input type="submit" value="Sign Up">
</form>
